{
  "name": "mouse-play-wrong",
  "version": "0.1.0",
  "description": "Enhanced mouse movement simulation for Playwright with human-like characteristics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:math": "jest src/tests/math/",
    "test:strategies": "jest src/tests/strategies/",
    "test:trusted-events": "ts-node src/scripts/run-trusted-events-test.ts",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "docs": "typedoc --out docs src",
    "benchmark": "ts-node src/benchmark/index.ts",
    "benchmark:math": "ts-node src/benchmark/math-performance.ts",
    "benchmark:direct": "node benchmark.js",
    "benchmark:threaded": "node threaded-benchmark.js",
    "visualize": "ts-node src/visualization/index.ts",
    "example:basic": "ts-node src/examples/basic-usage.ts",
    "example:symbolic": "ts-node src/examples/symbolic-math-extraction.ts",
    "example:context": "ts-node simple_context/ContextExample.ts",
    "example:context-visualize": "ts-node simple_context/ContextVisualizer.ts",
    "example:context-integration": "ts-node simple_context/MousePlayWrongWithContext.ts",
    "server:python": "cd native/python && python math_server.py",
    "build:cpp": "node-gyp configure && node-gyp build",
    "build:cpp:optimized": "node-gyp rebuild --verbose",
    "build:cpp:debug": "node-gyp rebuild --debug",
    "build:fortran": "cd native/fortran && make",
    "build:natives": "npm run build:cpp && npm run build:fortran",
    "build:all": "npm run build && npm run build:natives",
    "copy:binaries": "mkdir -p native/bin/cpp native/bin/fortran && cp -v build/Release/mouse_math.node native/bin/cpp/ || cp -v build/Debug/mouse_math.node native/bin/cpp/ || echo 'No C++ binary found' && cp -v native/fortran/fortran_physics.node native/bin/fortran/ || echo 'No Fortran binary found'",
    "prepublishOnly": "npm run build:all && npm run copy:binaries"
  },
  "keywords": [
    "playwright",
    "mouse",
    "movement",
    "automation",
    "simulation",
    "human-like",
    "bezier",
    "physics",
    "trajectory"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mousePlayWrong.git"
  },
  "dependencies": {
    "node-addon-api": "^6.1.0",
    "playwright": "^1.33.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "chai": "^5.2.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  }
}
